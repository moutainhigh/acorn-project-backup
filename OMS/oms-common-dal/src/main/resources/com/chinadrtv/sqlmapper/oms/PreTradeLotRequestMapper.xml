<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chinadrtv.dal.oms.dao.PreTradeLotRequestDao">

	<select id="findLastSuccess" parameterType="java.lang.Integer" resultType="PreTradeLotRequest">
		select * from acoapp_oms.pre_trade_lot_request p where p.source_id = #{sourceId} 
			<![CDATA[	
			where rownum <= 1
			]]>
			order by p.create_date desc 
	</select>
</mapper>