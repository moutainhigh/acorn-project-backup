<decorators defaultdir="/decorators">
    <excludes>
        <pattern>/40*.jsp</pattern>
        <pattern>/*ajax=true*</pattern>
        <pattern>/resources/*</pattern>
        <pattern>*/json/*</pattern>
        <pattern>/static/*</pattern>
    </excludes>
    <decorator name="default" page="default.jsp">
        <pattern>/wel</pattern>
        <pattern>/admin/*</pattern>
        <pattern>/company/*</pattern>
        <pattern>/order/*</pattern>
        <pattern>/orderPreprocessing/*</pattern>
        <pattern>/orderFreeback/*</pattern>        
        <pattern>/taobao/*</pattern>
        <pattern>/jingdong/*</pattern>
        <pattern>/chama/*</pattern>
        <pattern>/amazon/*</pattern>
        <pattern>/shipment/*</pattern>
        <pattern>/orderhist/*</pattern>
        <pattern>/orderhistNew/*</pattern>
        <pattern>/addreMaintain/*</pattern>
        <pattern>/carrier/*</pattern>
        <pattern>/freight/*</pattern>
        <pattern>/report/*</pattern>
        <pattern>/logisticsStatus/*</pattern>
        <pattern>/lower/*</pattern>

    </decorator>
    <decorator name="default_body" page="default_body.jsp">
        <pattern>*/selector</pattern>
        <pattern>*/selector/*</pattern>
    </decorator>
    <!-- 
	<decorator name="default_outside" page="default_outside.jsp">
         <pattern>/orderhist/*</pattern>
    </decorator>
 -->
</decorators>
